{
  "plugins": [
    "syntax-dynamic-import",
    "transform-object-rest-spread",
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs",
        "dynamic-import-node"
      ]
    },
    "production": {
      "presets": [
        ["es2015", { "modules": false }],
        "es2017"
      ],
      "plugins": [
        "transform-regenerator",
        "transform-runtime"
      ],
    }
  }
}
