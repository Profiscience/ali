{
  "name": "ali",
  "version": "0.0.0",
  "description": "The Other knockout-spa \"Framework\"",
  "scripts": {
    "dev": "NODE_ENV=development fly build",
    "prod": "NODE_ENV=production fly build",
    "start": "fly serve",
    "test": "jest",
    "debug": "node --debug-brk ./node_modules/.bin/jest --runInBand",
    "gh-pages": "yarn prod && rm -r docs && cp -r dist docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caseyWebb/ali.git"
  },
  "keywords": [
    "knockout",
    "knockoutjs",
    "framework",
    "spa",
    "mini-framework"
  ],
  "author": "Casey Webb <notcaseywebb@gmail.com>",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/caseyWebb/ali/issues"
  },
  "homepage": "https://github.com/caseyWebb/ali#readme",
  "dependencies": {
    "axios": "^0.17.0",
    "babel-runtime": "^6.23.0",
    "bulma": "^0.6.0",
    "jquery": "^3.2.1",
    "knockout": "^3.4.2",
    "knockout-punches": "^0.5.1",
    "ko-component-router": "5.0.1",
    "ko-contrib-fns": "^0.0.5",
    "ko-contrib-utils": "^0.4.0",
    "ko-querystring": "^2.0.2",
    "lodash": "^4.17.4",
    "toprogress": "^0.1.3"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.8.1",
    "babel": "^6.23.0",
    "babel-core": "^6.24.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "next",
    "babel-plugin-dynamic-import-node": "^1.0.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2017": "^6.22.0",
    "chalk": "^2.3.0",
    "compression-webpack-plugin": "^1.0.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.0",
    "escape-html": "^1.0.3",
    "eslint": "^4.9.0",
    "eslint-config-profiscience": "^2.0.4",
    "eslint-loader": "^1.7.1",
    "file-loader": "^1.1.5",
    "fly": "^2.0.5",
    "fly-clear": "^1.0.1",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.3.0",
    "jest": "^21.2.1",
    "ko-component-tester": "^3.2.1",
    "markdown-loader": "^2.0.0",
    "node-sass": "^4.5.2",
    "regenerator-runtime": "^0.11.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.4.2"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage",
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "testURL": "http://localhost:8080/",
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.html$": "<rootDir>/test/transformers/html.js"
    },
    "moduleDirectories": [
      "node_modules",
      "web_modules",
      "test"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|sass)$": "<rootDir>/test/__mocks__/style.js",
      "\\.(woff|ttf|eot|jpe?g|png|gif|svg)$": "<rootDir>/test/__mocks__/file.js"
    }
  }
}
